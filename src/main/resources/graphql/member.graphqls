schema {
    query: Query,
    mutation: Mutation,
}

type Mutation {
    registerMember(memberId: String!, memberEmail: String!, memberNumber: String!): MemberDto
    deleteMember(memberId: String!): Boolean
}

type Query{
	findMember(memberId: String!): MemberDto
	allFindMembers: [MemberDto]
}

type MemberDto {
	memberSn: Int!
	memberId: String!
	memberEmail : String!
	memberNumber : String!
}